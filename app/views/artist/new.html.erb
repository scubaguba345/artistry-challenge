<h1>New Artist Form</h1>

<%if flash[:my_errors]%>
    <% flash[:my_errors].each do |error|%>
    <%= error %>
<%end%>
<%end%>

<%=form_for @artist do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :age%>
    <%= f.text_field :age%>

    <%= f.label :title%>
    <%= f.text_field :title%>   


    <%= f.submit%>

<%end%>